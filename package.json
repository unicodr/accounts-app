    "@types/debug": "0.0.30",
    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "should": "^13.1.3",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  },
